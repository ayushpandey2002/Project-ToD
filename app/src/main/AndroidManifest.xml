<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bawp.todo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/todologo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/todologo2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Todoister">
        <activity
            android:name="com.bawp.todo.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bawp.todo.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Todoister.NoActionBar">

        </activity>
    </application>

</manifest>